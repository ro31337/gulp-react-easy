{
  "name": "gulp-react-easy",
  "version": "0.0.6",
  "description": "Gulp plugin to simplify React development with ES6",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run ava",
    "ava": "ava --require babel-register",
    "build": "babel ./src/index.js --out-file ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ro31337/gulp-react-easy.git"
  },
  "keywords": [
    "gulpplugin",
    "react.js",
    "reactjs",
    "es6",
    "react",
    "gulp"
  ],
  "author": "Roman Pushkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ro31337/gulp-react-easy/issues"
  },
  "homepage": "https://github.com/ro31337/gulp-react-easy",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-register": "^6.7.2"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "gulp-util": "^3.0.7",
    "object-assign": "^4.0.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "ava": {
    "files": [
      "test/index-test.js"
    ]
  }
}
